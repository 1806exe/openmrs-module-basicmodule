<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="@MODULE_PACKAGE@">

	<class name="PrintingQueue" table="printing_queue">

		<id name="printingQueueId" type="int" column="printing_queue_id" 
			unsaved-value="0">
				<generator class="native" />
		</id>

		<property name="payload" type="java.lang.String" 
			not-null="true" length="16777215" />
		
		<property name="status" type="int" />
		
		<property name="datePrinted" column="date_printed" 
			type="java.util.Date" not-null="false" length="19" />
		
		<many-to-one name="creator" class="org.openmrs.User" not-null="true">
			<column name="creator" />
		</many-to-one>

		<property name="dateCreated" type="java.util.Date" 
			column="date_created" not-null="true" length="19" />

	</class>
</hibernate-mapping>
